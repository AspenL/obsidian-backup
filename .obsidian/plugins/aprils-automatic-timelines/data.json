{
  "dateDisplayFormat": "{month} the {day} of {year} AR",
  "dateParserGroupPriority": "year,month,day,season,episode",
  "dateParserRegex": "(?<year>-?[0-9]*)-(?<month>-?[0-9]*)-(?<day>-?[0-9]*)-(?<season>-?[0-9]*)-(?<episode>-?[0-9]*)",
  "eventRenderToggleKey": "aat-render-enabled",
  "inlineEventEndOfBodyMarker": "%%aat-event-end-of-body%%",
  "markdownBlockTagsToFindSeparator": ",",
  "metadataKeyEventBodyOverride": "aat-event-body",
  "metadataKeyEventEndDate": "aat-event-end-date",
  "metadataKeyEventPictureOverride": "aat-event-picture",
  "metadataKeyEventStartDate": "aat-event-start-date",
  "metadataKeyEventTimelineTag": "timelines",
  "metadataKeyEventTitleOverride": "aat-event-title",
  "noteInlineEventKey": "aat-inline-event",
  "applyAdditonalConditionFormatting": true,
  "lookForCalendariumSpanEvents": true,
  "lookForInlineEventsInNotes": true,
  "lookForTagsForTimeline": false,
  "stylizeDateInline": false,
  "bodyFontSize": -1,
  "dateFontSize": -1,
  "titleFontSize": -1,
  "dateTokenConfiguration": [
    {
      "minLeght": 4,
      "name": "year",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    },
    {
      "minLeght": 2,
      "name": "month",
      "type": "STRING",
      "displayWhenZero": true,
      "formatting": [
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 1
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Teanar"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 2
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Kyro"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 3
            }
          ],
          "conditionsAreExclusive": true,
          "format": "Zatai"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 4
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Ondus"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 5
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Dedrath"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 6
            }
          ],
          "conditionsAreExclusive": false,
