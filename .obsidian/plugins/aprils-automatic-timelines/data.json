      ],
      "hideSign": false,
      "dictionary": [
        ""
      ]
    },
    {
      "minLeght": 1,
      "name": "day",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 1
            },
            {
              "condition": "EQUAL",
              "value": 21
            },
            {
              "condition": "EQUAL",
              "value": 31
            }
          ],
          "conditionsAreExclusive": true,
          "format": "{value}st"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 2
            },
            {
              "condition": "EQUAL",
              "value": 22
            },
            {
              "condition": "EQUAL",
              "value": 32
            }
          ],
          "conditionsAreExclusive": true,
