              "value": 3
            },
            {
              "condition": "NOTEQUAL",
              "value": 21
            },
            {
              "condition": "NOTEQUAL",
              "value": 22
            },
            {
              "condition": "NOTEQUAL",
              "value": 23
            },
            {
              "condition": "NOTEQUAL",
              "value": 31
            },
            {
              "condition": "NOTEQUAL",
              "value": 32
            },
            {
              "condition": "NOTEQUAL",
              "value": 33
            }
          ],
          "conditionsAreExclusive": false,
          "format": "{value}th"
        }
      ],
      "hideSign": false
    },
    {
      "minLeght": 1,
      "name": "season",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [
        {
          "evaluations": [],
          "conditionsAreExclusive": false,
          "format": "S{value}"
        }
      ],
      "hideSign": false
