        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 7
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Vanath"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 8
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Zanet"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 9
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Azath"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 10
            }
          ],
          "conditionsAreExclusive": false,
          "format": "Sin"
        },
        {
          "evaluations": [
            {
              "condition": "EQUAL",
              "value": 11
            }
